{
    "name": "lcframework/framework",
    "description": "The framework to the LastChaos CMS.",
    "keywords": [
        "lcframework",
        "laravel",
        "framework"
    ],
    "homepage": "https://github.com/lcframework/framework",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "marcj/topsort": "^2.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "LCFramework\\Framework\\": "src"
        },
        "files": [
            "helpers/settings.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LCFramework\\Framework\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LCFramework\\Framework\\LCFrameworkServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
